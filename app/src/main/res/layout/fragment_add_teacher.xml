<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.admin.AdminDashboardFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="16dp">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_first_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="First Name"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_first_name"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:backgroundTint="#ECE6F0"
            android:drawableStart="@drawable/ic_email"
            android:singleLine="true"
            android:text="Satish"
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_first_name" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_middle_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Middle Name"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_first_name" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_middle_name"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:backgroundTint="#ECE6F0"
            android:drawableStart="@drawable/ic_email"
            android:singleLine="true"
            android:text="Dnyaneshwar"
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_middle_name" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_last_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Last Name"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_middle_name" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_last_name"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:backgroundTint="#ECE6F0"
            android:drawableStart="@drawable/ic_email"
            android:singleLine="true"
            android:text="Bodake"
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_last_name" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_teacher_designation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Teacher Designation"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_last_name" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_teacher_designation"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:backgroundTint="#ECE6F0"
            android:singleLine="true"
            android:text="Senior Teacher"
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_teacher_designation" />


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_teacher_qualification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Teacher Qualification"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_teacher_designation" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_teacher_qualification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#ECE6F0"
            android:singleLine="true"
            android:text="B com"
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_teacher_qualification" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_teacher_dob"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Date of Birth"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_teacher_qualification" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_teacher_dob"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:backgroundTint="#ECE6F0"
            android:drawableStart="@drawable/ic_email"
            android:singleLine="true"
            android:text=""
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_teacher_dob" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_teacher_experience"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Teacher Experience"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_teacher_dob" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_teacher_experience"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:backgroundTint="#ECE6F0"
            android:drawableStart="@drawable/ic_email"
            android:singleLine="true"
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:textSize="16sp"
            android:text="12"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_teacher_experience" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_teacher_gender"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Teacher Gender"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_teacher_experience" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_teacher_gender"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:backgroundTint="#ECE6F0"
            android:drawableStart="@drawable/ic_email"
            android:singleLine="true"
            android:text="Male"
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_teacher_gender" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_date_of_joining"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Teacher Date of Joining"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_teacher_gender" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_date_of_joining"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:backgroundTint="#ECE6F0"
            android:drawableStart="@drawable/ic_email"
            android:singleLine="true"
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_date_of_joining" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_phone_number"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Teacher Phone Number"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_date_of_joining" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_phone_number"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:backgroundTint="#ECE6F0"
            android:drawableStart="@drawable/ic_email"
            android:singleLine="true"
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:text="8600381118"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_phone_number" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_email"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Teacher Email"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_phone_number" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_email"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:backgroundTint="#ECE6F0"
            android:drawableStart="@drawable/ic_email"
            android:singleLine="true"
            android:text="a@a.com"
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_email" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_salary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Teacher Salary"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_email" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_salary"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:backgroundTint="#ECE6F0"
            android:drawableStart="@drawable/ic_email"
            android:singleLine="true"
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:textSize="16sp"
            android:text="25000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_salary" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_status"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Teacher Status"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_salary" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/ev_status"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:backgroundTint="#ECE6F0"
            android:drawableStart="@drawable/ic_email"
            android:singleLine="true"
            android:textColor="#0C092A"
            android:textColorHint="#858494"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_status" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_add"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:text="Add Teacher"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ev_status"
            app:layout_constraintBottom_toBottomOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>